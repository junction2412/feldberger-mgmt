                    </padding>
                </StackPane>
                <StackPane alignment="TOP_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="8">
                    <children>
                        <Label fx:id="notes" text="notes"/>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
            </children>
        </GridPane>
        <VBox GridPane.rowIndex="3">
            <children>
                <StackPane>
                    <children>
                        <Label text="%label.customer.dashboard.transactions"/>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
                <StackPane alignment="BOTTOM_LEFT">
                    <children>
                        <Button fx:id="newTransaction" mnemonicParsing="false"
                                text="%button.customer.dashboard.new.transaction"/>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
                <StackPane>
                    <children>
                        <TableView fx:id="transactions">
                            <columns>
                                <TableColumn fx:id="transactionId" prefWidth="75.0"
                                             text="%table.customer.dashboard.id"/>
                                <TableColumn fx:id="transactionDescription" prefWidth="75.0"
                                             text="%table.customer.dashboard.description"/>
                                <TableColumn fx:id="transactionCreated" prefWidth="75.0"
                                             text="%table.customer.dashboard.created"/>
                                <TableColumn fx:id="transactionLastModified" prefWidth="75.0"
                                             text="%table.customer.dashboard.last.modified"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
            </children>
        </VBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <StackPane alignment="TOP_LEFT">
                    <children>
                        <Button fx:id="back" mnemonicParsing="false" text="%button.customer.dashboard.back"/>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
                <StackPane alignment="TOP_LEFT">
                    <children>
                        <Button fx:id="editCustomer" mnemonicParsing="false"
                                text="%button.customer.dashboard.edit.customer"/>
                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
            </children>
