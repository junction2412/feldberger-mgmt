                    </children>
                    <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                    </padding>
                </StackPane>
                <StackPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <children>
                        <Label fx:id="landlinePhoneNumber" text="landlinePhoneNumber"/>
                    </children>
